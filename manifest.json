{
  "manifest_version": 2,
  "name": "K12",
  "description": "A rebranding of the K12.com student panel.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://*.k12.com/*"],
      "css": ["style.css"],
      "js": ["grades.js"]
    }
  ],
  "web_accessible_resources": [
       "images/front-center-promo.png",
       "images/background.jpg",
       "images/header.jpg",
       "images/nav_bg.png",
       "font/typicons.css",
       "font/typicons.eot",
       "font/typicons.svg",
       "font/typicons.ttf",
       "font/typicons.woff"
       
     ]
}